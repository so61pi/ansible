#!/usr/bin/zsh

# print -s sudo sh -c "'apt update && apt upgrade && apt autoremove'"
print -s sudo sh -c "'zypper update && zypper dist-upgrade && zypper ps -s'"
print -s "zypper install package"
print -s "zypper in package"
print -s "zypper remove package"
print -s "zypper rm package"
print -s "zypper rm --clean-deps package"
print -s "zypper update"
print -s "zypper up"
print -s "zypper dist-upgrade"
print -s "zypper dup"
print -s "zypper removerepo repo"
print -s "zypper ps -s"
print -s "zypper list-updates"
print -s "zypper search 'usb*'"
print -s "zypper se 'usb*'"
print -s "zypper info usbutils"
print -s "zypper repos --uri"
print -s "zypper lr --uri"

print -s "git config --global user.name 'Example'"
print -s "git config --global user.email 'example@example.com'"
print -s "git config --list"
print -s "git init"
print -s "git clone <url>"
print -s "git clone --recurse-submodules <url>"
print -s "git add ."
print -s "git add --all ."
print -s "git commit -m ''"
print -s "git commit --all -m ''"
print -s "git pull"
print -s "git pull --rebase"
print -s "git pull --squash"
print -s "git -C dir pull"
print -s "git push"
print -s "git reset <commit>"
print -s "git reset --hard <commit>"
print -s "git ls-files | grep a"
print -s "git branch"
print -s "git branch --remote"
print -s "git branch name"
print -s "git branch --delete name"
print -s "git branch -d name"
print -s "git branch --delete --force name"
print -s "git branch -D name"
print -s "git branch --move old new"
print -s "git branch -m old new"
print -s "git branch --list 'feature/*'"
print -s "git branch -l 'feature/*'"
print -s "git rm -- file"
print -s "git checkout -- file"
print -s "git checkout master"
print -s "git checkout -b branch"
print -s "git diff"
print -s "git diff --staged"
print -s "git diff HEAD~3 HEAD -- file.txt"
print -s "git difftool"
print -s "git difftool --staged"
print -s "git log"
print -s "git log --graph"
print -s "git stash"
print -s "git stash pop"
print -s "git stash drop"
print -s "git stash list"
print -s "git submodule update"
print -s "git submodule update --recursive"
print -s "git submodule update --recursive --init"
print -s "git merge bugfix"
print -s "git merge --squash bugfix"
print -s "git clean -df"
print -s "git clean -xdf"
print -s "git reflog"
print -s "git instaweb"
print -s "gst"
print -s "ga ."
print -s "gcmsg ''"
print -s "gd"
print -s "gfg"
print -s "glgg"

print -s "docker build --tag <name:tag> ."
print -s 'docker run --env "TERM=xterm" --init --tty --volume $(pwd):/code --workdir /code <image> bash'
print -s "docker exec --tty --interactive --privileged <container> bash"
print -s "docker ps"
print -s "docker image ls"
print -s "docker container ls"
print -s "docker rmi <image>"
print -s "docker system prune"
print -s "docker system prune --all"
print -s "docker save -o file.tar image"
print -s "docker load -i file.tar"
print -s 'docker export "$(docker create --name ubuntu-18.04 ubuntu:18.04 true)" > ubuntu-18.04.tar'
print -s "docker rm ubuntu-18.04"

print -s "docker-compose up"
print -s "docker-compose up --build"
print -s "docker-compose down"

print -s "systemctl status"
print -s "systemctl status docker"
print -s "systemctl start docker"
print -s "systemctl stop docker"
print -s "systemctl restart docker"
print -s "systemctl enable docker"
print -s "systemctl disable docker"
print -s "systemctl edit docker"
print -s "systemctl cat docker"
print -s "systemd-analyze"
print -s "systemd-analyze blame"
print -s "systemd-analyze plot"
print -s "journalctl --unit ssh.service --follow"
print -s "journalctl --unit ssh.service --boot"

print -s "systemd-nspawn -b -M ubuntu-18.04"
print -s "systemd-nspawn -a -M ubuntu-18.04 /bin/bash"

print -s "resolvectl query example.com"
print -s "resolvectl query 8.8.8.8"
print -s "resolvectl status"

print -s "machinectl shell ubuntu-18.04 /bin/bash"
print -s "machinectl start ubuntu-18.04"
print -s "machinectl login ubuntu-18.04"
print -s "machinectl stop ubuntu-18.04"
print -s "machinectl reboot ubuntu-18.04"
print -s "machinectl status ubuntu-18.04"
print -s "machinectl bind --read-only --mkdir src dst"
print -s "machinectl copy-to src dst"
print -s "machinectl copy-from src dst"
print -s "machinectl enable ubuntu-18.04"
print -s "machinectl disable ubuntu-18.04"
print -s "machinectl list-images"
print -s "machinectl clean"
print -s "machinectl export-tar ubuntu-18.04 ubuntu-18.04.tar.xz"
print -s "machinectl import-tar ubuntu-18.04.tar ubuntu-18.04"
print -s "machinectl import-fs dir/ ubuntu-18.04"
print -s "machinectl remove ubuntu-18.04"
print -s "machinectl read-only ubuntu-18.04"
print -s "machinectl clone src dst"
print -s "machinectl rename old new"

print -s "ssh-copy-id user@host"
print -s "ssh-copy-id -i ~/.ssh/key.pub user@host"
print -s "ssh-copy-id 'user@host -p 22'"
print -s "sshfs -o uid=0,gid=0,default_permissions,reconnect,no_readahead,cache=yes,kernel_cache,compression=no,Cipher=arcfour,ro admin@example.com:/dir /dir"
print -s "fusermount -u /dir"
print -s "ssh -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no admin@example.com"
print -s "ssh -p 443 admin@example.com"
print -s "ssh -N -f -L 9000:example.com:80 admin@server.com"
print -s "ssh -N -f -L '*:9000:example.com:80' admin@server.com"
print -s "ssh -N -f -R 9000:localhost:3000 admin@server.com"
print -s "ssh -N -f -R 9000:address:3000 admin@server.com"
print -s "ssh -N -f -D localhost:3000 admin@server.com"

print -s "scp file user@server:/file"
print -s "scp user@server:/file file"
print -s "scp -r dir user@server:/dir"
print -s "scp -r user@server:/dir dir"
print -s "scp user1@server1:/file1 user2@server2:/file2"
print -s "scp -r user1@server1:/dir1 user2@server2:/dir2"

print -s "cargo init"
print -s "cargo init --bin"
print -s "cargo build"
print -s "cargo build --release"
print -s "cargo run"
print -s "cargo run --release"
print -s "cargo test"
print -s "cargo clean"
print -s "cargo clean --release"
print -s "cargo fmt --all"
print -s "cargo doc"
print -s "cargo doc --release"

print -s "go build"
print -s "go clean"
print -s "go env"
print -s "go fmt ./..."
print -s "go run"
print -s "got vet ./..."

print -s "g++ -std=c++17 -Wall -Wextra -Werror -O3 -pedantic -pthread -o main main.cpp"
print -s "g++ -S main.cpp"

print -s "rsync --dry-run --delete --archive --recursive --progress --verbose srcdir/ dstdir"
print -s "rsync --delete --archive --recursive --progress --verbose srcdir/ dstdir"

print -s "find dir/ -type f -name '*.log'"
print -s "find dir/ -type f -name '.*'"
print -s "find dir/ -type f -empty -delete"
print -s "find dir/ -type f -executable"
print -s "find dir/ -type d -empty -delete"
print -s "find dir/ -user root"

print -s "tar -cvSf archived.tar dir/"
print -s "tar -cvSf compressed.tar.xz --xz dir/"
print -s "tar -cvSf compressed.tar.gz --gzip dir/"
print -s "tar -cvSf compressed.tar.bz2 --bzip2 dir/"
print -s "tar -xvf archived.tar"
print -s "tar -xvf compressed.tar.xz --xz"
print -s "tar -xvf compressed.tar.gz --gzip"
print -s "tar -xvf compressed.tar.bz2 --bzip2"
print -s "tar -xvf archived.tar -C dir/"

print -s "wget https://example.com/"
print -s "wget -O example.html https://example.com/"
print -s "wget -i links.txt"
print -s "curl https://example.com/"
print -s "curl -o example.html https://example.com/"
print -s "curl --proxy yourproxy:port https://example.com/"
print -s "curl --location https://example.com/"

print -s "ip addr"
print -s "ip route"
print -s "ip neigh"
print -s "ip link set eth0 up"
print -s "ip link set eth0 down"
print -s "ss --tcp --all"
print -s "ss --tcp"
print -s "ss -t -a"

print -s "dig example.com"
print -s "nslookup example.com"
print -s "traceroute example.com"

print -s "cp src dst"
print -s "cp --archive src dst"
print -s "cp -a src dst"
print -s "mv src dst"
print -s "ln --symbolic --force --no-dereference src dst"
print -s "ln -sfn src dst"

print -s "chmod +r file"
print -s "chmod -r file"
print -s "chmod +x file"
print -s "chmod -x file"
print -s "chmod 664 file"
print -s "chmod 755 script"
print -s "chmod 1755 suscript"

print -s "chown root file"
print -s "chown root: file"
print -s "chown :root file"
print -s "chown nobody: file"
print -s "chown nobody:nogroup file"

print -s "md5sum file"
print -s "sha1sum file"
print -s "sha256sum file"
print -s "sha512sum file"

print -s "seq 10"
print -s "seq 0 10"
print -s "seq 0 2 10"
print -s "seq --separator , 0 10"
print -s "seq -s , 0 10"
print -s "seq --equal-width 0 10"
print -s "seq -w 0 10"

print -s "date"
print -s "date '+%F.%H-%M-%S'"
print -s "date '+%Y-%m-%d_%H-%M-%S'"

print -s "tree -d"
print -s "tree -f"
print -s "tree -h --du"
print -s "tree -h"
print -s "tree -L 3"
print -s "tree"

print -s "chattr +i ~/.gitconfig"
print -s "chattr -i ~/.gitconfig"
print -s "lsattr ~/.gitconfig"

print -s "sysctl fs.inotify.max_user_watches"
print -s "sysctl fs.inotify.max_user_watches=1048576"

print -s "python -m SimpleHTTPServer 8080"
print -s "python3 -m http.server 8080 --directory /tmp"

print -s "ping 8.8.8.8"
print -s "ping 1.1.1.1"

print -s "ps aux"
print -s "ps axjf"
print -s "top"
print -s "htop"

print -s "mount"
print -s "mount -o loop file.iso dir"

print -s "dd bs=512k status=progress if=/dev/urandom of=/dev/null"
print -s "df -h"
print -s "lsblk"
print -s "sync"

print -s "echo {0..3} | xargs mkdir"
print -s "du -h | sort -h"
print -s "free -h"
print -s "id3v2 -D **/*.mp3"
print -s "lscpu"
print -s "lsof"
print -s "lspci"
print -s "lsusb"
print -s "picocom -b 115200 /dev/ttyUSB0"
print -s "sleep 1"
print -s "tail -F file.log"
print -s "time cal"
print -s "uname -a"
print -s "uptime"
print -s "chroot ubuntu/ /bin/bash"

print -s "cat /proc/cmdline"
print -s "cat /proc/cpuinfo"
print -s "cat /proc/meminfo"
print -s "cat /proc/uptime"
print -s "cat /proc/version"
print -s "cat /proc/vmstat"

print -s "cat /etc/passwd"
print -s "cat /etc/group"
print -s "cat /etc/profile"
print -s "cat /etc/hosts"
print -s "cat /etc/resolv.conf"
print -s "cat /etc/hostname"

print -s "vim /etc/sysctl.d/99-sysctl.conf"
print -s "vim /etc/gitlab-runner/config.toml"
print -s "vim /etc/systemd/system/gitlab-runner.service"
print -s "vim ~/.ssh/config"
print -s "vim ~/.ssh/known_hosts"
